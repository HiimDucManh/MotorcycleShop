<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Background Colors -->
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#f3f3f3"/>
    <SolidColorBrush x:Key="SecundaryBackgroundColor" Color="#f9f9f9"/>
    <Color x:Key="PrimaryBackground">#f3f3f3</Color>
    <Color x:Key="SecundaryBackground">#020F16</Color>
    <Color x:Key="SecundaryBackground1">#434344</Color>
    <SolidColorBrush x:Key="BackgroundListItemModel" Color="#020F16" Opacity="0.7"/>
    <SolidColorBrush x:Key="BackgroundListItemModel1" Color="#020F16" Opacity="0.2"/>

    <!-- Primary Colors -->
    <SolidColorBrush x:Key="PrimaryRedColor" Color="#E32431"/>
    <SolidColorBrush x:Key="PrimaryGrayColor" Color="#BFBFC4"/>

    <!-- Secundary Colors -->
    <SolidColorBrush x:Key="SecundaryRedColor" Color="#E32431" Opacity="0.7"/>
    <SolidColorBrush x:Key="SecundaryGrayColor" Color="#EEEEEE"/>

    <!-- Typograhy Colors -->
    <SolidColorBrush x:Key="TextPrimaryColor" Color="#020F16"/>
    <SolidColorBrush x:Key="TextSecundaryColor" Color="#434344"/>

    <!-- Icons Colors -->
    <SolidColorBrush x:Key="ActionIconColor" Color="#434344"/>
</ResourceDictionary>