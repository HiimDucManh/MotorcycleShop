<Page x:Class="FinalProject.View.Customer.ImagePriceService"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FinalProject.View.Customer"
      mc:Ignorable="d" 
      Title="ImagePriceService"
      Style="{StaticResource Page_Style_RightLeft}">

    <Border>
        <Border.Background>
            <ImageBrush x:Name="ImgPriceService"
                ImageSource="/Assets/Avatars/Image.png"
                Stretch="Uniform">
            </ImageBrush>
        </Border.Background>
    </Border>
</Page>
